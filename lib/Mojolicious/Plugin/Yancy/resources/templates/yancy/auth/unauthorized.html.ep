% layout 'yancy/auth';
<main class="container-fluid">
    <div class="row">
        <div class="col">
            <h1>Unauthorized</h1>
            <p>You are not authorized to view this page.
            % if ( my $route = stash 'login_route' ) {
                %= link_to $route, 'Please log in'
            % }
        </div>
    </div>
</main>
